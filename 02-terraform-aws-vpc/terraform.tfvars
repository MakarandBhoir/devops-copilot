// Define the AWS region
region = "us-east-1"

// Define the CIDR block for the VPC
cidr_block = "10.0.0.0/16"

// Define the CIDR block for the public subnet
public_subnet_cidr_block = "10.0.0.0/24"

// Define the availability zone for the public subnet
public_subnet_availability_zone = "us-east-1a"

// Define the CIDR block for the private subnet
private_subnet_cidr_block = "10.0.1.0/24"

//Define the availability zone for the private subnet
private_subnet_availability_zone = "us-east-1b"

